{
  "name": "textrawl",
  "version": "0.1.0",
  "description": "Personal Knowledge MCP Server - semantic search over your documents from Claude",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "tsx watch --env-file=.env src/index.ts",
    "build": "tsc && node esbuild.config.mjs",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "inspector": "npx @modelcontextprotocol/inspector http://localhost:3000/mcp",
    "setup": "bash scripts/setup.sh",
    "convert": "tsx scripts/cli/convert.ts",
    "convert:mbox": "tsx scripts/cli/converters/mbox.ts",
    "convert:eml": "tsx scripts/cli/converters/eml.ts",
    "convert:takeout": "tsx scripts/cli/converters/takeout.ts",
    "convert:html": "tsx scripts/cli/converters/html.ts",
    "upload": "tsx scripts/cli/upload.ts",
    "ui": "tsx scripts/ui/server.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.24.0",
    "@supabase/supabase-js": "^2.45.0",
    "cors": "^2.8.5",
    "express": "^5.0.0",
    "express-rate-limit": "^7.4.0",
    "file-type": "^21.2.0",
    "helmet": "^8.1.0",
    "mammoth": "^1.8.0",
    "multer": "^2.0.2",
    "openai": "^4.70.0",
    "pdf-parse": "^1.1.1",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.0",
    "@types/jsdom": "^21.0.0",
    "@types/mailparser": "^3.4.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.0.0",
    "@types/sanitize-html": "^2.13.0",
    "@types/turndown": "^5.0.0",
    "cli-progress": "^3.12.0",
    "commander": "^12.0.0",
    "esbuild": "^0.24.0",
    "glob": "^10.3.0",
    "gray-matter": "^4.0.3",
    "ical.js": "^2.0.0",
    "jsdom": "^24.0.0",
    "mailparser": "^3.6.0",
    "node-mbox": "^2.0.0",
    "p-limit": "^5.0.0",
    "sanitize-html": "^2.12.0",
    "tsx": "^4.19.0",
    "turndown": "^7.1.0",
    "turndown-plugin-gfm": "^1.0.2",
    "typescript": "^5.7.0",
    "unzipper": "^0.12.0"
  },
  "keywords": [
    "mcp",
    "rag",
    "knowledge-base",
    "semantic-search",
    "claude"
  ],
  "author": "Jeff",
  "license": "MIT"
}
